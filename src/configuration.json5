{
  // 音频可视化配置
  audioVisualizationConfig: {
    // 是否开启音频可视化
    audioVisualizationEnabled: true,
    // 音频条颜色
    barColor: 'rgb(255, 255, 255)',
    // 音频条透明度
    barOpacity: 0.65,
    // 是否开启音频条阴影效果
    barShadowEnabled: true,
    // 音频条阴影颜色
    barShadowColor: 'rgb(255, 128, 192)',
    // 音频条的阴影扩散程度
    barShadowBlur: 50,
    // 音频条的宽度系数
    barWidthMultiplier: 0.8,
    // 音频条的初始高度
    barHeightInit: 2,
    // 音频条限制高度（限制值为屏幕高度X此数）
    barHeightLimit: 0.5,
    // 音频条Y轴偏移量
    barYOffset: 0,
    // 插值因子（推荐0.0~0.5之间，越大越平滑）
    lerpFactor: 0.5
  },
  // 花瓣飘落效果配置
  petalFlakeConfig: {
    // 是否开启花瓣飘落效果
    petalFlakeEnabled: true,
    // 花瓣的最大尺寸（实际尺寸为：这个值 * 0.5到1的随机数）
    petalMaxSize: 40,
    // 花瓣的风速（实际速度为：这个值 / 尺寸）
    petalWindSpeed: 40,
    // 花瓣飘落角度（90度为垂直向下）
    petalWindAngle: 135,
    // 花瓣的下落速度（实际速度为：尺寸 * 这个值）
    petalFallSpeed: 2,
    // 花瓣的透明度
    petalOpacity: 0.5,
    // 花瓣的数量
    petalCount: 20,
    // 是否开启花瓣阴影效果
    petalShadowEnabled: true,
    // 花瓣的阴影颜色及透明度
    petalShadowColor: 'rgb(255, 128, 192)',
    // 花瓣阴影的扩散程度
    petalShadowBlur: 10
  },
  // 时钟配置
  clockConfig: {
    // 是否开启时钟
    clockEnabled: true,
    // 时钟大小占浏览器窗口的比例
    sizeOfWindow: 0.6,
    // 时钟字体颜色
    clockFontColor: 'rgb(255, 255, 255)',
    // 时钟字体阴影颜色
    clockFontShadowColor: 'rgb(255, 128, 192)',
    // 时钟字体阴影扩散程度
    clockFontShadowBlur: 10,
    // 是否开启时钟背景模糊效果
    clockBackdropFilterEnabled: true,
    // 时钟背景模糊程度
    clockBackdropFilterBlur: 5,
    // 是否开启时钟律动效果
    clockMotionEnabled: true
  }
}
